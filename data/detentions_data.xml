<odoo>
    <data>
        <record id="sequence_detentions_invoice" model="ir.sequence">
            <field name="name">Detentions: Sequence folio</field>
            <field name="implementation">no_gap</field>
            <field name="code">sequence.detentions.folio</field>
            <field name="active">1</field>
            <field name="prefix">SVI/%(y)s%(month)s</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="False" name="company_id"/>
        </record>
    </data>
</odoo>